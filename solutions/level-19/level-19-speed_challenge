    OUTBOX  
    BUMPDN   0
    JUMPZ    d
    JUMP     c
d:
    OUTBOX  
    JUMP     b
e:
    COPYTO   0
f:
    OUTBOX  
    BUMPUP   0
    JUMPZ    g
    JUMP     f
g:
h:
    OUTBOX  
    JUMP     a


